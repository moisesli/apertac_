<%- model_class = Documento -%>
<div class="page-header">
  <h1><%=t '.title', :default => model_class.model_name.human.titleize %></h1>
</div>

<dl class="dl-horizontal">
  <dt><strong><%= model_class.human_attribute_name(:titulo) %>:</strong></dt>
  <dd><%= @documento.titulo %></dd>
  <dt><strong><%= model_class.human_attribute_name(:descripcion) %>:</strong></dt>
  <dd><%= @documento.descripcion %></dd>
  <dt><strong><%= model_class.human_attribute_name(:contenido) %>:</strong></dt>
  <dd><%= @documento.contenido %></dd>
  <dt><strong><%= model_class.human_attribute_name(:tipo_extension) %>:</strong></dt>
  <dd><%= @documento.tipo_extension %></dd>
  <dt><strong><%= model_class.human_attribute_name(:tipo_documento) %>:</strong></dt>
  <dd><%= @documento.tipo_documento %></dd>
  <dt><strong><%= model_class.human_attribute_name(:visible) %>:</strong></dt>
  <dd><%= @documento.visible %></dd>
  <dt><strong><%= model_class.human_attribute_name(:archivo1) %>:</strong></dt>
  <dd><%= @documento.archivo1 %></dd>
  <dt><strong><%= model_class.human_attribute_name(:archivo2) %>:</strong></dt>
  <dd><%= @documento.archivo2 %></dd>
  <dt><strong><%= model_class.human_attribute_name(:archivo3) %>:</strong></dt>
  <dd><%= @documento.archivo3 %></dd>
  <dt><strong><%= model_class.human_attribute_name(:archivo4) %>:</strong></dt>
  <dd><%= @documento.archivo4 %></dd>
  <dt><strong><%= model_class.human_attribute_name(:archivo5) %>:</strong></dt>
  <dd><%= @documento.archivo5 %></dd>
  <dt><strong><%= model_class.human_attribute_name(:imagen1) %>:</strong></dt>
  <dd><%= @documento.imagen1 %></dd>
  <dt><strong><%= model_class.human_attribute_name(:imagen2) %>:</strong></dt>
  <dd><%= @documento.imagen2 %></dd>
  <dt><strong><%= model_class.human_attribute_name(:imagen3) %>:</strong></dt>
  <dd><%= @documento.imagen3 %></dd>
  <dt><strong><%= model_class.human_attribute_name(:imagen4) %>:</strong></dt>
  <dd><%= @documento.imagen4 %></dd>
  <dt><strong><%= model_class.human_attribute_name(:imagen5) %>:</strong></dt>
  <dd><%= @documento.imagen5 %></dd>
</dl>

<%= link_to t('.back', :default => t("helpers.links.back")),
              documentos_path, :class => 'btn btn-default'  %>
<%= link_to t('.edit', :default => t("helpers.links.edit")),
              edit_documento_path(@documento), :class => 'btn btn-default' %>
<%= link_to t('.destroy', :default => t("helpers.links.destroy")),
              documento_path(@documento),
              :method => 'delete',
              :data => { :confirm => t('.confirm', :default => t("helpers.links.confirm", :default => 'Are you sure?')) },
              :class => 'btn btn-danger' %>
